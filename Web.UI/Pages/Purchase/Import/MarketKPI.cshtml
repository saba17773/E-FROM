@page
@model Web.UI.Pages.Purchase.Import.MarketKPIModel

@using Infrastructure.Models

@{
  ViewData["Title"] = "Market KPI";

  ViewData["Breadcrumb"] = new List<BreadcrumbModel> {
    new BreadcrumbModel { Text = "Import", Link = "javascript:void();" },
    new BreadcrumbModel { Text = "Import Market KPI" }
  };
}

<div class="card">
  <div class="card-header">
    Market KPI
  </div>
  <div class="card-body">
    <form method="post" enctype="multipart/form-data">
      <input type="file" name="TemplateUpload" />

      <button type="submit" class="btn btn-success">Submit</button>
    </form>
  </div>
</div>
