@page "/Queing/Dashboard/All"
@model Web.UI.Pages.Queing.Dashboard.AllModel
@using Interfaces
@using Infrastructure.Models

@inject IAuthService authService

@{
    Layout = "_LayoutQueing";
}
@{
    ViewData["Title"] = "Main Dashboard";
    ViewData["status"] = "0";

    ViewData["Breadcrumb"] = new List<BreadcrumbModel> {
        new BreadcrumbModel { Text = "Home", Link = "/Queing/Home" },
        new BreadcrumbModel { Text = "Main Dashboard" }
    };
}
<style>
    .container2 {
        border-radius: 5px;
        background-color: white;
        padding: 15px;
    }

    .overlay {
        background: #f9f6f3;
        display: none;
        position: absolute;
        top: 0;
        right: 0;
        bottom: 0;
        left: 0;
        opacity: 0.5;
    }

    .imgloading {
        position: absolute;
        left: 50%;
        top: 80%;
        width: 5%;
    }

    input::-webkit-inner-spin-button {
        -webkit-appearance: none;
        margin: 0;
    }
</style>

<form method="post" enctype="multipart/form-data" id="form_submit">
    <div class="container2">
        <br />
        <div class="row row-cols-1 row-cols-md-2">
            <div class="col mb-3">
                <div class="card text-center text-black bg-warning mb-3" style="max-width: 18rem;">
                    <a href="/Queing/Dashboard/WeighInOut" class="card-body btn btn-warning">
                        <b class="card-title">
                            <i class="fa fa-television"></i>
                            Weigh In-Out
                        </b>
                    </a>
                </div>
            </div>

            <div class="col mb-3">
                <div class="card text-center text-black bg-warning mb-3" style="max-width: 18rem;">
                    <a href="/Queing/Dashboard/Index" class="card-body btn btn-warning">
                        <b class="card-title">
                            <i class="fa fa-television"></i>
                            OVER SEA
                        </b>
                    </a>
                </div>
            </div>

            <div class="col mb-3">
                <div class="card text-center text-black bg-warning mb-3" style="max-width: 18rem;">
                    <a href="/Queing/Dashboard/Bay" class="card-body btn btn-warning">
                        <b class="card-title">
                            <i class="fa fa-television"></i>
                            BAY
                        </b>
                    </a>
                </div>
            </div>

        </div>

        <br />
        <div class="row">
            <a href="/Queing/Home" class="btn btn-light">
                <i class="fa fa-reply"></i> Back
            </a>
        </div>

    </div>
</form>